{
  "name": "quickcommands",
  "description": "Some helpfull quickcommands tool to use with Salesforce",
  "version": "0.0.0",
  "author": "Paolo Zirotti",
  "bugs": "https://github.com/paolino7/quickcommands/issues",
  "dependencies": {
    "@babel/parser": "^7.8.4",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/errors": "^1.2.2",
    "@salesforce/command": "^3.1.0",
    "@salesforce/core": "^2.19.1",
    "@types/object-assign-deep": "^0.4.0",
    "@types/request": "^2.48.4",
    "json2csv": "^5.0.6",
    "object-assign-deep": "^0.4.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "shelljs": "^0.8.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/test": "^1.2.5",
    "@salesforce/dev-config": "^1.6.0",
    "@types/chai": "^4.2.9",
    "@types/json2csv": "^5.0.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.2",
    "@types/request-promise": "^4.1.45",
    "chai": "^4.2.0",
    "globby": "^11.0.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/paolino7/quickcommands",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "user": {
        "description": "Commands to work with users."
      },
      "data": {
        "description": "Commands to work with data"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "paolino7/quickcommands",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
